{
  "name": "frc-pose-view",
  "displayName": "FRC Pose View",
  "description": "",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:java"
  ],
  "main": "./out/extension.js",
  "bin": {
    "frc-eval": "./out/cli.js"
  },
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "frc-pose-view-explorer",
          "title": "FRC Pose View",
          "icon": "resources/icon.png"
        }
      ]
    },
    "views": {
      "frc-pose-view-explorer": [
        {
          "type": "webview",
          "id": "frc-pose-view.fieldView",
          "name": "Field View"
        }
      ]
    },
    "commands": [
      {
        "command": "frc-pose-view.evaluateExpression",
        "title": "FRC Pose View: Evaluate Expression"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "eval": "npm run compile && node ./out/cli.js",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "chevrotain": "^11.0.3"
  }
}